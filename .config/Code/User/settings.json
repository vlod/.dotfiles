{
    "telemetry.enableTelemetry": false,
    "editor.minimap.enabled": false,
    "terminal.integrated.rendererType": "dom",
    "window.zoomLevel": 2,
    "editor.tabSize": 2,
    "git.autofetch": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "workbench.activityBar.visible": true,
    "editor.dragAndDrop": false,
    "window.menuBarVisibility": "default",
    "breadcrumbs.enabled": true,
    "[xml]": {
        "editor.defaultFormatter": "fabianlauer.vs-code-xml-format"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.startupEditor": "none",
    "git.suggestSmartCommit": false,
    "git.untrackedChanges": "separate",
    "editor.renderLineHighlight": "all",
    "editor.fontSize": 11,
    "debug.console.fontSize": 11,
    "markdown.preview.fontSize": 11,
    "terminal.integrated.fontSize": 11,
    "mochaExplorer.debuggerConfig": "",
    "mochaExplorer.env": {
        "NODE_ENV": "test"
    },
    "jestrunner.jestPath": "./node_modules/.bin/jest",
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "python.pythonPath": "/usr/bin/python3",
    "mdb.confirmRunAll": false,
    "explorer.confirmDelete": false,
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "server": "localhost",
            "port": 5432,
            "driver": "PostgreSQL",
            "name": "signatry_dev",
            "database": "signatry_dev",
            "username": "user",
            "password": "pass"
        }
    ],
    "json.schemas": [
        {
            "fileMatch": ["cypress.json"],
            "url": "https://on.cypress.io/cypress.schema.json"
        }
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "debug.onTaskErrors": "abort",
    "editor.bracketPairColorization.enabled": true,
    "editor.suggest.preview": true,
}